<%= form_for(@user) do |f| %>
<div>
  <%= f.label :name %>
  <%= f.text_field :name %>
</div>
<div>
  <%= f.label :date_of_birth %>
  <%= f.text_field :date_of_birth %>
</div>
<div>
  <%= f.label :my_workout %>
  <%= f.text_area :my_workout %>
</div>
<div>
  <%= f.label :my_diet %>
  <%= f.text_area :my_diet %>
</div>
<div>
  <%= f.submit %>
</div>
<% end %>
